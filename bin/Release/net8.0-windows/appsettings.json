{
  "ConnectionStrings": {
    "Default": "Server=localhost;Database=SampleDB;Integrated Security=true;TrustServerCertificate=true;"
  },
  "Bot": {
    "MaxRows": 1000,
    "Provider": "OpenAI",
    "OpenAI": {
      "ApiKey": "",
      "Model": "gpt-4",
      "BaseUrl": "https://api.openai.com/v1"
    },
    "Gemini": {
      "ApiKey": "",
      "Model": "gemini-pro"
    },
    "Ollama": {
      "BaseUrl": "http://localhost:11434",
      "Model": "llama2"
    }
